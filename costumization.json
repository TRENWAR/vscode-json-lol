{
  "terminal.integrated.tabs.location": "left",
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "editor.fontLigatures": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.centerLayout": false,
  "vsicons.dontShowNewVersionMessage": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.fontWeight": "normal",
  "editor.minimap.autohide": true,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.includeComments": true,
  "workbench.colorCustomizations": {
    "[Noctis Minimus]": {
      "statusBar.background": "#046fc7",
      "statusBar.foreground": "#ece8e8",
      "editor.selectionBackground": "#ff84d634",
      // "editor.selectionHighlightBorder": "#ffffffc2",
     "editor.selectionHighlightBackground": "#ffffff2d"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Noctis Minimus]": {
      "comments": "#4db2ec",
      // "comments": "#d6d6d6e5",
      "numbers": "#5096ff",
      "keywords": {
        "fontStyle": "italic"
      }
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "[Noctis Minimus]": {
      "enabled": true,
      "rules": {
        "class": "#e5c07b",
        "parameter": {
          "foreground": "#f09383d5",
          "fontStyle": "italic"
        },
        "*.decorator": "#46a1ecd7",
      }
    }
},

  "explorer.sortOrder": "type",
  "terminal.integrated.gpuAcceleration": "auto",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.typeCheckingMode": "off",
  "vsicons.presets.jsonOfficial": true,
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.layoutControl.enabled": false,
  "glassit.alpha": 240,
  "python.defaultInterpreterPath": "C:\\Users\\HP\\AppData\\Local\\Programs\\Python\\Python310\\python.exe",
  "liveServer.settings.donotVerifyTags": true,
  "css.completion.completePropertyWithSemicolon": false,
  "terminal.integrated.cursorBlinking": true,
  "prettier.semi": false,
  "emmet.excludeLanguages": [
    "markdown",
    "css"
  ],
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "editor.fontSize": 15,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "",
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "info"
  ],
  "errorLens.enabled": true,
  "python.terminal.activateEnvironment": false,
  "comments.useRelativeTime": false,
  "workbench.activityBar.visible": true,
  "window.menuBarVisibility": "compact",
  "http.proxyAuthorization": null,
  "terminal.integrated.fontSize": 12,
  "oneDarkPro.vivid": false,
  "workbench.colorTheme": "Noctis Minimus",
  "workbench.iconTheme": "material-icon-theme"
}
